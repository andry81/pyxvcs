# DESCRIPTION:
#   `*.HUB_ABBR` uses to select what template command scripts to use for generation and
#   `*.PROJECT_PATH_LIST` uses to select where to generate
#

SVN_SF.HUB_ROOT:      svn.code.sf.net/p
SVN_SF.HUB_ABBR:      sf
SVN_SF.USER:          user

GIT_GH.HUB_ROOT:      github.com
GIT_GH.HUB_ABBR:      gh
GIT_GH.REPO_OWNER:    owner
GIT_GH.USER:          user
GIT_GH.SVNSSH.USER:   ${SVN_SF.USER}
GIT_GH.EMAIL:         user@mail.com

GIT_BB.HUB_ROOT:      bitbucket.org
GIT_BB.HUB_ABBR:      bb
GIT_BB.REPO_OWNER:    owner
GIT_BB.USER:          user
GIT_BB.SVNSSH.USER:   ${SVN_SF.USER}
GIT_BB.EMAIL:         user@mail.com

GIT_GL.HUB_ROOT:      gitlab.com
GIT_GL.HUB_ABBR:      gl
GIT_GL.REPO_OWNER:    owner
GIT_GL.USER:          user
GIT_GL.SVNSSH.USER:   ${SVN_SF.USER}
GIT_GL.EMAIL:         user@mail.com

SVN_SF.PROJECT_PATH_LIST:
  - project_group/project

GIT_GH.PROJECT_PATH_LIST:
  - project_group/project

GIT_BB.PROJECT_PATH_LIST:
  - project_group/project

GIT_GL.PROJECT_PATH_LIST:
  - project_group/project
